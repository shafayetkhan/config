URxvt.perl-lib: /home/sylvain/.urxvt/urxvt-perls
URxvt.scrollBar: False
URxvt.geometry: 80x24
URxvt.font: xft:DejaVu Sans Mono for Powerline:size=10
URxvt.letterSpace: -2
URxvt.perl-ext-common: default,keyboard-select,url-select,clipboard
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-g: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.underline: true
Xft*dpi: 96
Xft*antialias: true
Xft*hinting: full
URxvt.transparent: true
URxvt.tintColor: white
URxvt.shading: 32
URxvt*fading: 26

